{
  "name": "frankx",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --config eslint.config.js 'app/**/*.{ts,tsx}' 'components/**/*.{ts,tsx}' 'lib/**/*.{ts,tsx}' --ignore-pattern 'app/design-lab/**' --ignore-pattern 'components/v0-variants/**'",
    "type-check": "tsc --noEmit",
    "gen:html": "node scripts/generate_interlinked_html.mjs",
    "gen:feed": "node scripts/generate_feed.mjs",
    "gen:search": "node scripts/generate_search_index.mjs",
    "content:index": "node scripts/generate-youtube-index.mjs",
    "vault:scan": "node scripts/generate-vault-manifest.mjs",
    "newsletter:index": "node scripts/generate-content-index.mjs",
    "prebuild": "npm run gen:html && npm run gen:feed && npm run gen:search && npm run content:index && npm run newsletter:index && npm run vault:scan",
    "roadmap:check": "node scripts/daily-roadmap-check.js",
    "watch": "node scripts/watch.mjs",
    "ai:scan": "node scripts/ai-trend-scanner.mjs",
    "content:validate": "node scripts/content-publisher.mjs",
    "schema:generate": "node scripts/generate-schema.mjs",
    "morning:ops": "npm run ai:scan && npm run roadmap:check",
    "publish:validate": "npm run content:validate && npm run schema:generate",
    "meta:generate": "node scripts/generate-meta.mjs",
    "pdf:build": "node scripts/build-pdf.mjs",
    "publish:artifact": "node scripts/publish.mjs",
    "qa:check": "node scripts/qa-gatekeeper.mjs",
    "factory:validate": "npm run content:validate && npm run qa:check",
    "factory:publish": "npm run qa:check && npm run publish:artifact",
    "diagrams:inventory": "node scripts/diagrams/inventory.mjs --write",
    "diagrams:migrate": "node scripts/diagrams/migrate-legacy.mjs",
    "diagrams:render": "node scripts/diagrams/render.mjs",
    "diagrams:guard": "node scripts/diagrams/guard.mjs",
    "diagrams:baseline": "node scripts/diagrams/guard.mjs --update-baseline",
    "ci:check": "npm run merge:gate && npm run lint && npm run content:validate && npm run diagrams:guard",
    "merge:gate": "npm run type-check && npm run claims:audit:strict && npm run links:check:ci",
    "links:check": "node scripts/check-links.mjs",
    "links:check:ci": "bash scripts/run-links-check-with-dev.sh",
    "claims:audit": "node scripts/audit-marketing-claims.mjs",
    "claims:audit:strict": "node scripts/audit-marketing-claims.mjs --strict",
    "agents:worktrees": "bash scripts/agents/setup-worktrees.sh",
    "agents:locks": "node scripts/agents/locks.mjs",
    "image:plan": "node scripts/image-ops/image-router.mjs plan",
    "image:status": "node scripts/image-ops/image-router.mjs status",
    "image:generate": "node scripts/image-ops/image-router.mjs generate",
    "image:matrix": "node scripts/image-ops/image-matrix.mjs run",
    "aiops:image:plan": "npm run image:plan",
    "starlight:image:plan": "npm run image:plan",
    "log:session": "node scripts/log-session.mjs",
    "sync:check": "bash scripts/pre-deploy-sync-check.sh",
    "sync:fix": "bash scripts/pre-deploy-sync-check.sh --sync",
    "sync:diff": "bash scripts/pre-deploy-sync-check.sh --diff",
    "predeploy": "npm run merge:gate && npm run sync:check"
  },
