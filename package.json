    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "three": "^0.182.0",
    "tldraw": "^4.4.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^11.12.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@terrastruct/d2": "^0.1.33",
    "@types/bcryptjs": "^2.4.6",
    "@types/lunr": "^2.3.7",
    "@types/node": "^22.10.2",
