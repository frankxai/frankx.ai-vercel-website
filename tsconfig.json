{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    ".next",
    "app/design-lab/**",
    "components/v0-variants/**",
    "tests",
    "_repo_remote_frankx_website",
    "agentic-creator-os-mcp",
    "agentic-creator-os-npm",
    "arcanea-opencode",
    "backups",
    "claude-agentic-creator-os",
    "frankx-oracle-genai-content",
    "sanity-frankx",
    "workshops-repo",
    "workshops",
    "reading-site",
    "orchestration",
    "pipelines",
    "skills",
    ".worktrees",
    "components/ai-architecture",
    "FrankX.AI - Vercel Website",
    "inbound",
    "lib/pipelines",
    "keystatic-frankx",
    "content-universe",
    "new-landing-page-backup",
    "brand",
    "playwright.config.ts",
    "vitest.config.ts",
    "vitest.setup.ts",
    "tina-frankx",
    "tina",
    ".tina",
    ".claude",
    ".agent",
    ".claude-skills",
    "docs",
    "scripts",
    "agentic-creator-os",
    "_archive",
    "v1-enterprise-backup"
  ]
}
